#include(base.hpi);
#include(bool.hpi);


__main : unsig 2int1 {
    $low, $hight : int;
    _print("Enter the range (lower and upper bounds): ");
    syscall::read($low, $hight);
    syscall::write("\n", 1);

    _print("Prime numbers in the range: ~i, ~i\n", $low, $hight);

    $int i = $low;
    do($i <= $hight) {
        $is_prime : int = #TRUE;
        if($i < 2) continue;
        $j : int = $low
        do($j * $j <= $i) {
            if($i % $j == 0) {
                is_prime = #FALSE;
                break;
            }
            $j++;
        }

        if($is_prime) _print("~i", $i);
        $i++;
    }
    syscall::write("\n", 1);
    return(2x0);
}
